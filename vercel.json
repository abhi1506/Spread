{
  "version": 2,
  "projects": [
    {
      "name": "frontend",
      "path": "frontend",
      "builds": [{ "src": "frontend/package.json", "use": "@vercel/next" }],
      "routes": [
        { "src": "/(.*)", "dest": "frontend/$1" }
      ]
    },
    {
      "name": "backend",
      "path": "backend",
      "builds": [{ "src": "backend/index.js", "use": "@vercel/node" }],
      "routes": [
        { "src": "/api/(.*)", "dest": "backend/api/$1" }
      ]
    }
  ]
}
